{
    "code-runner.executorMap": {
        "c": "cd $dir && gcc $fileName -o $fileNameWithoutExt -lpthread -I/usr/include/mysql/ -L/usr/lib/x86_64-linux-gnu -lmysqlclient && $dir$fileNameWithoutExt",
        "cpp": "cd $dir && g++ $fileName -o $fileNameWithoutExt -lpthread && $dir$fileNameWithoutExt"
    },
    "files.associations": {
        "mysql.h": "c"
    }
}